{
    "manifest_version": 3,
    "name": "My ibdocs extension",
    "version": "1.0",
    "description": "A brief description",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "storage",
      "activeTab",
      "webNavigation",
      "tabs",
      "scripting"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*",
      "http://repo4udpz5ip2sk7m4c2aazxroaivsyw2dj5ctsnyfx2ami2ayhn43ad.onion/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["contentScript.js"]
      }
  ]
    
  }
  