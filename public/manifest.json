{
    "manifest_version": 3,
    "name": "My ibdocs extension",
    "version": "1.0",
    "description": "A brief description",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "storage",
      "activeTab",
      "webNavigation",
      "tabs",
      "scripting"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    }
    
  }
  